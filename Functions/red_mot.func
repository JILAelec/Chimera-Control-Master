def red_mot(loadTime):
  on: red3d
  dac: red3dpow .4
  dac: x1coil xcoilval
  dac: y1coil ycoilval
  dac: z1coil 1.65


  dac: lowermotcoil 1
  dac: uppermotcoil 1
  

  t += loadTime
  off: red3d
  on: red3dnarrow
  on: trigger 
  t += .1
  off: trigger

  t += 30


  off: red3dnarrow

  
  %dac: red3dpow .15
  %t += 30
 
  dac: red3dpow .4
  t += .01
  
