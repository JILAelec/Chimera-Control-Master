  dac: x2coil xcoilval
  dac: x1coil xcoilval
  dac: y2coil ycoilval
  dac: y1coil ycoilval
  dac: z2coil zcoilval
  dac: z1coil zcoilval
  dac: lowermotcoil 1
  dac: uppermotcoil 1
  

  t += loadTime
  %off: red3d
  %on: red3dnarrow
  %t += 30
  %off: red3dnarrow

  
  %dac: red3dpow .15
  %t += 30
 
